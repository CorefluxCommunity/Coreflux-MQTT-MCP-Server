# TLS Configuration (disabled for development)
MQTT_USE_TLS=false

# Coreflux Copilot API Configuration
DO_AGENT_API_KEY=your_development_api_key_here

# Logging Configuration (debug for development)
LOG_LEVEL=DEBUG
